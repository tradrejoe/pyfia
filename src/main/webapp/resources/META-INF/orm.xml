<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
		version="1.0">

	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>

	<package>com.uxl.sites.pyfia</package>

	<entity class="ClassFinancial">
		<table name="cls_fin" />
		<attributes>
			<id name="name" />
			<basic name="name"><column name="nm"/></basic>
			<basic name="altname"><column name="nm2"/></basic>
			<basic name="description"><column name="disp"/></basic>
			<basic name="attribute0"><column name="attr0"/></basic>
			<basic name="attribute1"><column name="attr1"/></basic>
			<basic name="attribute2"><column name="attr2"/></basic>
			<basic name="attribute3"><column name="attr3"/></basic>
		</attributes>
	</entity>

</entity-mappings>
